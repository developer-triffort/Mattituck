<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:paddingRight="10dp" 
    android:paddingBottom="20dp"
    >       
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        android:paddingTop="10dp"
        android:paddingLeft="10dp"
        android:onClick="onOver"
        >
        <ImageButton
           android:id="@+id/on_o"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:background="@drawable/overdrive" 
           android:onClick="onOver"
           />
			<TextView
			    android:id="@+id/on_o_a"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="@string/sv_over"
			    android:textColor="@android:color/black"
                android:textSize="@dimen/title"  
                android:textStyle="bold"
                android:layout_toRightOf="@+id/on_o"
                android:layout_marginLeft="10dp"
                android:onClick="onOver"
			    />
			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="@string/sv_over_below"
			    android:textColor="@android:color/black"
                android:textSize="@dimen/menu"  
                android:layout_toRightOf="@+id/on_o"
                android:layout_below="@+id/on_o_a"
                android:layout_marginLeft="10dp"
                android:onClick="onOver"
			    />
    </RelativeLayout>
